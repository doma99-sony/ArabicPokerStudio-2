<?xml version="1.0" encoding="UTF-8"?>
<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <title>Gold Coin Texture</title>
  <defs>
    <linearGradient id="coinGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#FFF9C4" />
      <stop offset="30%" stop-color="#FFC107" />
      <stop offset="70%" stop-color="#FFA000" />
      <stop offset="100%" stop-color="#FF8F00" />
    </linearGradient>
    <linearGradient id="edgeGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" stop-color="#FF6F00" />
      <stop offset="50%" stop-color="#FFA000" />
      <stop offset="100%" stop-color="#FF8F00" />
    </linearGradient>
  </defs>
  
  <!-- Coin Base -->
  <circle cx="100" cy="100" r="95" fill="url(#coinGradient)" stroke="url(#edgeGradient)" stroke-width="5"/>
  
  <!-- Inner Circle -->
  <circle cx="100" cy="100" r="75" fill="none" stroke="#FF8F00" stroke-width="2" opacity="0.8"/>
  
  <!-- Ankh Symbol (Egyptian Symbol of Life) -->
  <path d="M100,40 
           L100,90
           C80,100 80,120 100,130
           C120,120 120,100 100,90
           Z" 
        fill="none" 
        stroke="#873600" 
        stroke-width="8"
        stroke-linejoin="round"
        stroke-linecap="round"/>
  <line x1="70" y1="130" x2="130" y2="130" stroke="#873600" stroke-width="8" stroke-linecap="round"/>
  
  <!-- Decorative Elements -->
  <circle cx="100" cy="100" r="25" fill="none" stroke="#873600" stroke-width="3" opacity="0.7"/>
  
  <!-- Coin Edge Details -->
  <circle cx="100" cy="100" r="95" fill="none" stroke="#873600" stroke-width="1.5" stroke-dasharray="2,4"/>
  
  <!-- Highlight -->
  <path d="M60,60 Q100,20 140,60" stroke="#FFFFFF" stroke-width="3" opacity="0.7" fill="none"/>
  <path d="M50,100 Q70,70 90,90" stroke="#FFFFFF" stroke-width="2" opacity="0.5" fill="none"/>
</svg>
